        <image bindtap='delete' data-tap='7' data-src='{{img7}}' class='img' src='../../static/images/zbicon011.png'></image>
        <image bindtap='view' data-tap='7' data-src='{{img7}}' class='img' src='../../static/images/shenbanicon01.png'></image>
      </view>
    </view>

  </view>

  <view class='flex-center-start-column'>
    <text class='tips fz15 fw700'>
      <text class='co-red'></text>原准予行政许可决定书和原效果图复印件
    </text>

    <view class='w100 pr'>
      <view bindtap='uploadFn1' data-tap='8' class="flex-center-column w100 content body fz14 co-99">
        <text wx-if="{{!img8}}">拍照上传</text>
        <image wx-if="{{!img8}}" style='height : 24px; width : 24px;' src='../../static/images/icon-tianjia.png'></image>
        <image wx-if="{{img8}}" mode='aspectFit' src='{{img8}}'></image>
      </view>

      <view wx-if="{{img8}}" class="flex-center w100 module fz14 co-99 pf">
        <image bindtap='uploadFn1' data-tap='8' data-src='{{img8}}' class='img' src='../../static/images/bid/add.png'></image>
        <image bindtap='delete' data-tap='8' data-src='{{img8}}' class='img' src='../../static/images/zbicon011.png'></image>
        <image bindtap='view' data-tap='8' data-src='{{img8}}' class='img' src='../../static/images/shenbanicon01.png'></image>
      </view>
    </view>
  </view>


  <view class='flex-center-start-column'>
    <view class='tips fz15 fw700 flex-center-between w100'>
      <text><text class='co-red'>*</text> 承诺书</text>
      <text bindtap='download' class='co-red'>下载承诺书模板</text>
    </view>
    <view class='w100 pr'>
      <view bindtap='uploadFn1' data-tap='9' class="flex-center-column w100 content body fz14 co-99">
        <text wx-if="{{!img9}}">拍照上传</text>
        <image wx-if="{{!img9}}" style='height : 24px; width : 24px;' src='../../static/images/icon-tianjia.png'></image>
