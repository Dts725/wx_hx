        <image bindtap='delete' data-tap='9' data-src='{{img9}}' class='img' src='../../static/images/zbicon011.png'></image>
        <image bindtap='view' data-tap='9' data-src='{{img9}}' class='img' src='../../static/images/shenbanicon01.png'></image>
