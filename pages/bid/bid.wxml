        <image bindtap='delete' data-tap='6' data-src='{{img6}}' class='img' src='../../static/images/zbicon011.png'></image>
        <image bindtap='view' data-tap='6' data-src='{{img6}}' class='img' src='../../static/images/shenbanicon01.png'></image>
      </view>

    </view>

  </view>


  <view class='flex-center-start-column'>
    <text class='tips fz15 fw700'>
      <text class='co-red'></text>相邻人同意设置的书面协议
    </text>

    <view class='w100 pr'>
      <view bindtap='uploadFn1' data-tap='7' class="flex-center-column w100 content body fz14 co-99">
        <text wx-if="{{!img7}}">拍照上传</text>
        <image wx-if="{{!img7}}" style='height : 24px; width : 24px;' src='../../static/images/icon-tianjia.png'></image>
