  <view class='white btn mt-1rem'>
    <view class='fw700 padt-1rem margin-bottom padr-1rem padl-1rem'>
      <text style='color:#333'>办事追踪</text>
    </view>
    <view class=' w100 flex-center-evenly margin-bottom'>
      <image mode='aspectFit' bindtap='routeBid' src='../../static/images/menu01-track.png'></image>
      <image mode='aspectFit' bindtap='routeProgress' src='../../static/images/menu02-track.png'></image>
      <image mode='aspectFit' class='pr' bindtap='questionOnline' src='../../static/images/menu03-track.png'>
        <view wx-if='flag' class='wechat pf'>
          <contact-button type="default-dark" size="20" session-from="tokens"></contact-button>
