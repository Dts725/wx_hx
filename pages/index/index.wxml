      <view bindtap='intellectConsultation' class='w25 prepare-item'>
        <view class='flex-center-column'>
          <image class='img' src='../../static/zizhu/gonglue3.png' mode='widthFix'></image>
          <text>智能咨询</text>
        </view>
      </view>
      <view bindtap='problemCommon' class='w25 prepare-item'>
        <view class='flex-center-column'>
          <image class='img' src='../../static/zizhu/gonglue4.png' mode='widthFix'></image>
          <text>常见问题</text>
        </view>
      </view>
