<view class='body'>
  <view class='online-banner'>
    <image class='w100' style='display:block;' mode='widthFix' src='../../static/images/index-banner.png'></image>
  </view>
  <view class='white'>
    <view class='fw700 padt-1rem margin-bottom padr-1rem padl-1rem'>
      <text style='color:#333'>准备攻略</text>
    </view>
    <view class=' w100 flex-center-warp flex-center-warp-circl margin-bottom'>
      <view bindtap='politicalRules' class='w25 prepare-item'>
        <view class='flex-center-column'>
          <image class='img' src='../../static/zizhu/gonglue1.png' mode='widthFix'></image>
          <text>政策法规</text>
        </view>
      </view>
      <view bindtap='guideReserve' class='w25 prepare-item'>
        <view class='flex-center-column'>
          <image class='img' src='../../static/zizhu/gonglue2.png' mode='widthFix'></image>
          <text>办事指南</text>
        </view>
      </view>
